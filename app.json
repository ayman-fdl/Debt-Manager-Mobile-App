{
  "expo": {
    "name": "Debt Manager",
    "slug": "dept-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#030712"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tom.debtmanager",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.tom.debtmanager",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#030712"
      },
      "permissions": [
        "READ_CONTACTS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Debt Manager to access your contacts to select people you owe or who owe you."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1a3426a-747c-477b-9c9e-310306559db4"
      }
    },
    "owner": "debt-manager"
  }
}